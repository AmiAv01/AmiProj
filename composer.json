{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.4.2",
        "guzzlehttp/guzzle": "^7.9.2",
        "inertiajs/inertia-laravel": "v2.0.0",
        "laravel/framework": "11.38.0",
        "laravel/sanctum": "v4.0.7",
        "laravel/tinker": "^v2.10.0",
        "moneyphp/money": "^v4.6",
        "spatie/laravel-query-builder": "^6.3.0",
        "tightenco/ziggy": "^1.0",
        "ext-intl": "*",
        "ext-bcmath": "*"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^v3.14.10",
        "fakerphp/faker": "^1.9.1",
        "knuckleswtf/scribe": "^4.39.0",
        "laravel/breeze": "^v2.1.5",
        "laravel/pint": "^v1.20.0",
        "laravel/sail": "^v1.40.0",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^v8.5.0",
        "pestphp/pest": "^v3.5.2",
        "pestphp/pest-plugin-laravel": "^v3.0.0",
        "spatie/laravel-ignition": "^2.9.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
